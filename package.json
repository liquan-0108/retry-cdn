{
  "name": "retry-cdn",
  "version": "0.1.1",
  "description": "Automatically switch the domain name when error",
  "main": "build/retry-cdn.js",
  "scripts": {
    "dev": "webpack server --hot",
    "build:to-h5": "webpack --mode development --output-path examples/demo-h5/utils/",
    "build:to-webpack": "webpack --mode development --output-path examples/demo-webpack/utils",
    "build:to-vite": "webpack --mode development --output-path examples/demo-vite/utils",
    "build:dev": "npm run build:to-h5 && npm run build:to-webpack && npm run build:to-vite",
    "build:pro": "webpack --mode production",
    "test": "jest"
  },
  "repository": {
    "type": "git",
